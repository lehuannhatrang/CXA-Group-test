{
  "name": "videohub_server",
  "version": "1.0.0",
  "description": "Backend for videohub",
  "main": "index.js",
  "scripts": {
    "clean-build": "npm run clean && mkdir -p ../dist/server &&  NODE_ENV=production PORT=3001 babel ./ --ignore node_modules -s -d ../dist/server",
    "build": "mkdir -p ../dist/server &&  NODE_ENV=production PORT=3001 babel ./ --ignore node_modules -s -d ../dist/server && cp -r package.json bin ../dist/server && cd ../dist/server && npm install",
    "start": "NODE_ENV=production PORT=3001 node bin/prod",
    "start-dev": "NODE_ENV=dev PORT=9001 node bin/dev",
    "clean": "rimraf ../dist/server"
  },
  "author": "Leehun",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "btoa": "^1.2.1",
    "child_process": "^1.0.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "csurf": "^1.10.0",
    "csv-write-stream": "^2.0.0",
    "deep-diff": "^1.0.2",
    "ejs": "^2.6.1",
    "email-templates": "2.0.0-beta.4",
    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    "express-session": "^1.17.0",
    "handlebars": "^4.7.6",
    "helmet": "^3.21.2",
    "https": "^1.0.0",
    "lodash": "^4.17.10",
    "log4js": "^3.0.5",
    "memory-streams": "^0.1.3",
    "mkdirp": "^0.5.1",
    "moment": "^2.22.2",
    "mongoose": "^5.7.7",
    "morgan": "^1.9.0",
    "multer": "^1.4.2",
    "node-cache": "^5.1.0",
    "node-fetch": "^2.6.0",
    "node-schedule": "^1.3.2",
    "nodemailer": "^6.1.1",
    "nodemailer-plugin-inline-base64": "^2.1.1",
    "passport": "^0.4.0",
    "passport-cas": "^0.1.1",
    "pg": "^8.5.1",
